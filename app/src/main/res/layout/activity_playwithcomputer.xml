<?xml version="1.0" encoding="utf-8"?>
<TableLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".TicTacToeActivity" android:gravity="center">

    <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center" android:id="@+id/title_table_row">
        <TextView
                android:text="Tic Tac Toc Game"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/title" android:gravity="center"
                android:textSize="30sp" android:textColor="@color/common_google_signin_btn_text_dark_focused"
                android:textStyle="bold"/>
    </TableRow>
    <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center">
        <ImageButton
                android:layout_width="0dp"
                android:id="@+id/imgButton1" android:layout_weight="1"
                android:scaleType="fitCenter" android:layout_height="100dp" android:onClick="imageButoonTapped"
                android:src="@drawable/back_img"/>
        <ImageButton
                android:layout_width="0dp"
                android:id="@+id/imgButton2" android:layout_weight="1"
                android:scaleType="fitCenter" android:layout_height="100dp" android:onClick="imageButoonTapped"
                android:src="@drawable/back_img"/>
        <ImageButton
                android:layout_width="0dp"
                android:id="@+id/imgButton3" android:layout_weight="1"
                android:scaleType="fitCenter" android:layout_height="100dp" android:onClick="imageButoonTapped"
                android:src="@drawable/back_img"/>
    </TableRow>
    <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center">
        <ImageButton
                android:layout_width="0dp"
                android:id="@+id/imgButton4" android:layout_weight="1"
                android:scaleType="fitCenter" android:layout_height="100dp" android:onClick="imageButoonTapped"
                android:src="@drawable/back_img"/>
        <ImageButton
                android:layout_width="0dp"
                android:id="@+id/imgButton5" android:layout_weight="1"
                android:scaleType="fitCenter" android:layout_height="100dp" android:onClick="imageButoonTapped"
                android:src="@drawable/back_img"/>
        <ImageButton
                android:layout_width="0dp"
                android:id="@+id/imgButton6" android:layout_weight="1"
                android:scaleType="fitCenter" android:layout_height="100dp" android:onClick="imageButoonTapped"
                android:src="@drawable/back_img"/>
    </TableRow>
    <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="center">
        <ImageButton
                android:layout_width="0dp"
                android:id="@+id/imgButton7" android:layout_weight="1"
                android:scaleType="fitCenter" android:layout_height="100dp" android:onClick="imageButoonTapped"
                android:src="@drawable/back_img"/>
        <ImageButton
                android:layout_width="0dp"
                android:id="@+id/imgButton8" android:layout_weight="1"
                android:scaleType="fitCenter" android:layout_height="100dp" android:onClick="imageButoonTapped"
                android:src="@drawable/back_img"/>
        <ImageButton
                android:layout_width="0dp"
                android:id="@+id/imgButton9" android:layout_weight="1"
                android:scaleType="fitCenter" android:layout_height="100dp" android:onClick="imageButoonTapped"
                android:src="@drawable/back_img"/>
    </TableRow>
    <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:gravity="center">
    </TableRow>
</TableLayout>